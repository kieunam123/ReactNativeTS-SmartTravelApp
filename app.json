{
  "expo": {
    "name": "Smart Travel",
    "slug": "smarttravel",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "@react-native-voice/voice",
        {
          "microphonePermission": "Cho phép Smart Travel truy cập microphone của bạn",
          "speechRecogntionPermission": "Allow Smart Travel to securely recognize user speech"
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.japfa.swinetest",
      "buildNumber": "2",
      "infoPlist": {
        "NSFaceIDUsageDescription": "Cho phép ứng dụng sử dụng FaceID/TouchID để xác thực đăng nhập",
        "NSLocationWhenInUseUsageDescription": "Cho phép ứng dụng sử dụng vị trí của bạn",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Cho phép ứng dụng sử dụng vị trí khi sử dụng",
        "NSLocationAlwaysUsageDescription": "Cho phép ứng dụng sử dụng vị trí của bạn khi chạy nền",
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.japfa.swinetest",
      "config": {
        "googleMaps": {
          "apiKey": ""
        }
      },
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": ""
      }
    }
  }
}
